package lakshmi;

public class condition {

	public static void main(String[] args) {
		int a=5,b=23,c=4;
		if(a<b&a<c)
		{
			System.out.println("a is small");
			
		}
		else if(b<c)
		
		{
			System.out.println("b is small");
		}
		else
		{
			System.out.println("c is small");
		}
			
			
			
			

	}

}
